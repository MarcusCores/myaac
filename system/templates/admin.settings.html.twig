<div class="card card-primary card-outline card-outline-tabs">
	<div class="card-header">
		<h5 class="m-0">Settings</h5>
	</div>
	<div class="card-body">
		<form method="post">
			<div class="row">
				<div class="col-md-12">
					<div class="box">
						<div class="box-body">
							<button name="save" type="submit" class="btn btn-primary">Save</button>
						</div>
						<br/>
						{{ settings|raw }}
					</div>
				</div>
			</div>
		</form>
	</div>
</div>
